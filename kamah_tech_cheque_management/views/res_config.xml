<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="res_config_settings_view_form_inherit_account" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="account.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[@id='import_bank_statement_camt']" position="after">
                <div class="col-12 col-lg-6 o_setting_box">
                    <div class="o_setting_left_pane" />
                    <div class="o_setting_right_pane">
                        <span class="o_form_label">Check Return Journal</span>
                        <div class="text-muted">
                                Checks Returned Journal
                            </div>
                        <div class="content-group">
                            <div class="row mt16">
                                <label for="check_return_journal" class="col-lg-3 o_light_label" />
                                <field name="check_return_journal" />
                            </div>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>

</odoo>